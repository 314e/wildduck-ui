{
	"name": "wildduck-ui",
	"version": "1.0.0",
	"description": "Administrative UI for wildduck IMAP server",
	"main": "index.js",
	"scripts": {
		"preinstall": "npm prune",
		"postinstall": "npm outdated || true",
		"project:install": "npm install && git submodule foreach npm install",
		"project:init": "git submodule update --init --recursive && npm run project:install",
		"webpack-dev-server": "webpack-dev-server",
		"webpack": "webpack",
		"generate-open-api": "cd ./submodules/wildduck-redux && npm run generate-open-api",
		"dev": "npm run generate-open-api && npm run webpack-dev-server -- --env.mode development",
		"production": "npm run generate-open-api && npm run test && npm run webpack -- --env.mode production",
		"serve": "serve dist -p 3000",
		"serve:prd": "npm run production && npm run serve",
		"prod:analyse": "npm run webpack -- --env.mode production --env.analyse true",
		"package-update": "npx ncu -u && npm install",
		"eslint": "eslint .",
		"eslint:fix": "eslint --fix . "
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/softwareartistry/wildduck-ui.git"
	},
	"keywords": [],
	"author": "Lakkanna Ramachandra Walikar <lakkannawalikar@gmail.com> (https://lakkanna.github.io/)",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/softwareartistry/wildduck-ui/issues"
	},
	"homepage": "https://github.com/softwareartistry/wildduck-ui#readme",
	"devDependencies": {
		"@types/react": "16.9.51",
		"@types/react-dom": "16.9.8",
		"@types/react-loadable": "5.5.3",
		"@types/react-redux": "7.1.9",
		"@typescript-eslint/eslint-plugin": "4.4.0",
		"@typescript-eslint/parser": "4.4.0",
		"awesome-typescript-loader": "5.2.1",
		"clean-webpack-plugin": "3.0.0",
		"copy-webpack-plugin": "5.1.1",
		"css-loader": "4.3.0",
		"eslint": "7.10.0",
		"eslint-plugin-react": "7.21.3",
		"file-loader": "6.1.0",
		"html-webpack-plugin": "4.5.0",
		"mini-css-extract-plugin": "0.11.3",
		"serve": "11.3.2",
		"style-loader": "1.3.0",
		"typescript": "4.0.3",
		"webpack": "4.44.2",
		"webpack-babel-multi-target-plugin": "2.5.0",
		"webpack-bundle-analyzer": "3.9.0",
		"webpack-cli": "3.3.12",
		"webpack-dev-server": "3.11.0",
		"webpack-merge": "5.1.4"
	},
	"dependencies": {
		"@ant-design/compatible": "1.0.8",
		"@ant-design/icons": "4.3.0",
		"@types/lodash": "4.14.162",
		"@types/react-router-dom": "5.1.6",
		"antd": "4.9.2",
		"braft-editor": "^2.3.9",
		"eml-format": "^0.6.1",
		"kea": "2.2.0",
		"less": "^3.12.2",
		"less-loader": "^7.0.2",
		"lodash": "4.17.20",
		"react": "16.13.1",
		"react-dom": "16.13.1",
		"react-highlight-words": "^0.16.0",
		"react-loadable": "5.5.0",
		"react-query": "^3.5.11",
		"react-redux": "7.2.1",
		"react-router-dom": "5.2.0",
		"redux": "4.0.5",
		"reselect": "4.0.0"
	}
}
